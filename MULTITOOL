echo ║
echo ║
echo ║
echo ╚2)System Remote
set /p input=... %BS%
if /i %input% EQU 1 call :Doxx menu
if /i %input% EQU 2 call :remote
if /i %input% EQU 7 call :1010

:Doxx menu
cls
:z
