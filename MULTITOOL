echo Enter 1 for IP Pullerr:

echo Enter 2 for Password Puller:

echo Enter 3 for Address Pullerr:

echo Enter Exit to go to multitool
echo  ---------------------------------
set /p ans="Enter program Number:"
if %ans%==Exit (
goto Start
)
if %ans%==1 (
goto a
)
if %ans%==2 (
goto b
)
if %ans%==3 (
goto c
)
if %ans%==4 (
goto d
)


:a
cls
echo You have enterd Discord Ip puller
set /p User="Enter Username: "

cls
 echo user found
 echo IP: 135.164.50.34
)
pause
cls
goto z

:b
cls
set /p User="Enter key: "
if %ans%==9562 (
goto b1
)

:b1
cls
echo You have enterd Discord Password puller
set /p User="Enter Username: "
cls
 echo user found
 echo password is 123xX2011leoXx321
pause
